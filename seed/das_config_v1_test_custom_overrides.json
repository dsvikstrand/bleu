{
  "version": 1,
  "notes": [
    "Smoke test config for CONTROL_PACK param overrides.",
    "Use with: --das --compose-controls --compose-prompts --das-config seed/das_config_v1_test_custom_overrides.json",
    "Forces CONTROL_PACK domain=custom + focus=custom so we can validate rendering and downstream behavior."
  ],
  "defaults": {
    "enabled": true,
    "kCandidates": 1,
    "maxAttempts": 1,
    "eval": ["structural", "bounds"],
    "onHardFail": "stop_run"
  },
  "nodes": {
    "CONTROL_PACK": {
      "kCandidates": 1,
      "maxAttempts": 1,
      "eval": ["structural", "bounds"],
      "params": {
        "force_domain": "custom",
        "force_domain_custom": "Longevity",
        "force_focus": "custom",
        "force_focus_custom": "Strength for busy beginners"
      }
    },
    "APPLY": {
      "enabled": false
    }
  }
}

